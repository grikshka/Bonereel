      <Label fx:id="lblMovieEndTime" layoutX="826.0" layoutY="673.0" text="--:--" />
      <Button layoutX="107.0" layoutY="699.0" mnemonicParsing="false" onAction="#clickPlay" prefHeight="37.0" prefWidth="84.0" text="Play/Stop" />
      <Slider fx:id="sldVolume" layoutX="675.0" layoutY="710.0" max="1.0" value="0.25" />
      <ToggleButton fx:id="btnMute" layoutX="571.0" layoutY="705.0" mnemonicParsing="false" onAction="#clickMute" prefHeight="25.0" prefWidth="84.0" text="Mute" />
   </children>
