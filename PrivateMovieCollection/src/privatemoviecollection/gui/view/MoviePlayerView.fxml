<AnchorPane id="AnchorPane" prefHeight="758.0" prefWidth="908.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="privatemoviecollection.gui.controller.MoviePlayerViewController">
